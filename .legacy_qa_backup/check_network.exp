#!/usr/bin/expect -f

set timeout 10
set password "101abn"

puts "=== Testing Network Connectivity ==="
spawn bash -c "ping -c 3 192.168.50.210 && echo '=== Testing SSH Port ===' && nc -zv -w 5 192.168.50.210 22 && echo '=== Testing Route ===' && traceroute -n 192.168.50.210"

expect {
    timeout {
        puts "\n❌ Network check timed out"
        exit 1
    }
    eof {
        puts "\n✅ Network check complete"
        exit 0
    }
}
