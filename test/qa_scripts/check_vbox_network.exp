#!/usr/bin/expect -f

set timeout 10
set password "101abn"

puts "=== Checking VirtualBox Network Configuration ==="
spawn bash -c "VBoxManage showvminfo 'Server' | grep -i 'NIC' && echo === DHCP === && VBoxManage list dhcpservers && echo === HOST INTERFACES === && VBoxManage list hostonlyifs"

expect {
    timeout {
        puts "\n❌ Check timed out"
        exit 1
    }
    eof {
        puts "\n✅ Check complete"
        exit 0
    }
}
