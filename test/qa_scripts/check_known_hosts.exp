#!/usr/bin/expect -f

set timeout 10
set password "101abn"

puts "=== Checking SSH Known Hosts Configuration ==="
spawn bash -c "echo === LOCAL KNOWN HOSTS === && cat ~/.ssh/known_hosts && echo === REMOTE KNOWN HOSTS === && ssh sblanken@192.168.50.210 'cat ~/.ssh/known_hosts'"

expect {
    "password:" {
        send "$password\r"
        exp_continue
    }
    timeout {
        puts "\n❌ Check timed out"
        exit 1
    }
    eof {
        puts "\n✅ Check complete"
        exit 0
    }
}
