#!/usr/bin/expect -f

set timeout 10
set password "101abn"

spawn ssh -v sblanken@192.168.50.210 "cat /etc/ssh/sshd_config && echo '=== PAM Config ===' && cat /etc/pam.d/sshd"

expect {
    "password:" {
        send "$password\r"
        exp_continue
    }
    timeout {
        puts "\n❌ Connection timed out"
        exit 1
    }
    eof {
        puts "\n❌ Connection closed"
        exit 1
    }
}
